{
  "build": {
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    }
  },
  "routes": [
    {
      "src": "/ton-connect/.*",
      "dest": "/api/ton-connect",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/donacije",
      "dest": "/donacije",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
}